{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iam:PutRolePolicy",
        "iam:PassRole",
        "iam:*Get*",
        "iam:*List*",
        "s3:*Get*",
        "s3:*List*",
        "s3:*Describe*",
        "eks:*",
        "ecr:*",
        "ec2:*",
        "route53:*",
        "logs:*",
        "cloudformation:*",
        "cloudwatch:*",
        "autoscaling:*",
        "elasticloadbalancing:*",
        "access-analyzer:ListPolicyGenerations",
        "eks-auth:AssumeRoleForPodIdentity",
        "sts:AssumeRoleWithWebIdentity",
        "logs:*",
        "kms:DescribeKey",
        "iam:DetachRolePolicy"
      ],
      "Resource": "*"
    }
  ]
}